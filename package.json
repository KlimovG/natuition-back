{
  "name": "natuition-back",
  "version": "0.0.1",
  "description": "Backend for the Natuition Web App",
  "main": "dist/app.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "cross-env NODE_ENV=production ts-node build/app.js",
    "dev": "nodemon src/app.ts",
    "lint": "eslint . --ext .ts",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "create-table": "npm run typeorm:cli -- migration:run",
    "drop-table": "npm run typeorm:cli -- migration:revert"
  },
  "author": "Klimov Georgii aka TraneDev",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "nodemon": "^2.0.19",
    "pg": "^8.8.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "apollo-server-express": "^3.10.2",
    "class-validator": "^0.13.2",
    "express": "^4.18.1",
    "graphql": "^15.7.2",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.9"
  }
}
